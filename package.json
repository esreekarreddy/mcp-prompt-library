{
  "name": "@esreekarreddy/ai-prompts",
  "version": "1.0.1",
  "description": "94+ curated AI prompts, workflows, and coding standards for developers. Battle-tested templates for PRDs, debugging, security audits, code reviews, and more.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./prompts": {
      "import": "./dist/prompts.js",
      "types": "./dist/prompts.d.ts"
    }
  },
  "files": [
    "dist",
    "prompts",
    "snippets",
    "templates",
    "skills",
    "instructions",
    "chains",
    "contexts",
    "examples",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "ai",
    "prompts",
    "prompt-library",
    "prompt-engineering",
    "llm",
    "claude",
    "gpt",
    "chatgpt",
    "openai",
    "anthropic",
    "coding-standards",
    "developer-tools",
    "productivity",
    "prd",
    "debugging",
    "code-review",
    "security-audit",
    "mcp",
    "cursor",
    "copilot"
  ],
  "author": {
    "name": "Sreekar Reddy",
    "url": "https://sreekarreddy.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esreekarreddy/mcp-prompt-library.git"
  },
  "homepage": "https://github.com/esreekarreddy/mcp-prompt-library#readme",
  "bugs": {
    "url": "https://github.com/esreekarreddy/mcp-prompt-library/issues"
  },
  "dependencies": {
    "glob": "^11.0.0",
    "gray-matter": "^4.0.3"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
